<article>
 <header>
	<h2>
		<%= @fuellog.log_date %>
		[ <%= link_to 'Edit', edit_fuellog_path(@fuellog) %> ]
	</h2>
 </header>
 
 <h3>Odometer:</h3>
 <p><%= @fuellog.odometer %></p>
 
 <p>Auto: <%= h(@fuellog.auto.title) rescue 'No auto found' %></p>
 
 <h3>PPG</h3>
 <p><%= @fuellog.ppg %></p>
 
 <h3>Gallons</h3>
 <p><%= @fuellog.gallons %></p>
 
 <h3>Total Cost</h3>
 <p><%= @fuellog.total_cost %></p>
 
</article>

<p><%= link_to 'Back', fuellogs_path %></p>